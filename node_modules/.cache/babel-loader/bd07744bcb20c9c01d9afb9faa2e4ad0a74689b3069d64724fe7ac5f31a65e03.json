{"ast":null,"code":"import RecipeCard from './RecipeCard.vue'; // We will create this next\n\nexport default {\n  name: 'RecipeSearch',\n  components: {\n    RecipeCard\n  },\n  data() {\n    return {\n      ingredientInput: '',\n      recipes: [],\n      loading: false,\n      searched: false // To show \"No recipes found\" only after a search attempt\n    };\n  },\n  methods: {\n    searchRecipes() {\n      // API call logic will go here in the next step\n      this.loading = true;\n      this.searched = true;\n      // Simulate API call for now\n      setTimeout(() => {\n        this.recipes = []; // Clear previous results\n        this.loading = false;\n        console.log('Searching for:', this.ingredientInput);\n        // Example: If 'test' is in input, show dummy data\n        if (this.ingredientInput.toLowerCase().includes('test')) {\n          this.recipes = [{\n            id: 1,\n            title: 'Test Chicken Dish',\n            ingredients: ['chicken', 'test ingredient'],\n            instructions: 'Cook it',\n            image: 'https://via.placeholder.com/150'\n          }, {\n            id: 2,\n            title: 'Test Veggie Meal',\n            ingredients: ['broccoli', 'test ingredient'],\n            instructions: 'Bake it',\n            image: 'https://via.placeholder.com/150'\n          }];\n        }\n      }, 1000);\n    }\n  }\n};","map":{"version":3,"names":["RecipeCard","name","components","data","ingredientInput","recipes","loading","searched","methods","searchRecipes","setTimeout","console","log","toLowerCase","includes","id","title","ingredients","instructions","image"],"sources":["/Users/srdemorais/Sandbox/dinner-time/srdemorais/dinner_time/recipe_frontend/src/components/RecipeSearch.vue"],"sourcesContent":["<template>\n  <div class=\"recipe-search\">\n    <h2>What ingredients do you have?</h2>\n    <div class=\"search-input\">\n      <input\n        type=\"text\"\n        v-model=\"ingredientInput\"\n        @keyup.enter=\"searchRecipes\"\n        placeholder=\"e.g., chicken, rice, onion\"\n      />\n      <button @click=\"searchRecipes\">Find Recipes</button>\n    </div>\n\n    <div v-if=\"loading\" class=\"loading-message\">\n      Loading recipes...\n    </div>\n\n    <div v-if=\"recipes.length > 0\" class=\"recipe-list\">\n      <RecipeCard v-for=\"recipe in recipes\" :key=\"recipe.id\" :recipe=\"recipe\" />\n    </div>\n    <div v-else-if=\"!loading && searched\" class=\"no-results-message\">\n      No recipes found with those ingredients. Try different ones!\n    </div>\n  </div>\n</template>\n\n<script>\nimport RecipeCard from './RecipeCard.vue'; // We will create this next\n\nexport default {\n  name: 'RecipeSearch',\n  components: {\n    RecipeCard\n  },\n  data() {\n    return {\n      ingredientInput: '',\n      recipes: [],\n      loading: false,\n      searched: false // To show \"No recipes found\" only after a search attempt\n    };\n  },\n  methods: {\n    searchRecipes() {\n      // API call logic will go here in the next step\n      this.loading = true;\n      this.searched = true;\n      // Simulate API call for now\n      setTimeout(() => {\n        this.recipes = []; // Clear previous results\n        this.loading = false;\n        console.log('Searching for:', this.ingredientInput);\n        // Example: If 'test' is in input, show dummy data\n        if (this.ingredientInput.toLowerCase().includes('test')) {\n            this.recipes = [\n                { id: 1, title: 'Test Chicken Dish', ingredients: ['chicken', 'test ingredient'], instructions: 'Cook it', image: 'https://via.placeholder.com/150' },\n                { id: 2, title: 'Test Veggie Meal', ingredients: ['broccoli', 'test ingredient'], instructions: 'Bake it', image: 'https://via.placeholder.com/150' }\n            ];\n        }\n      }, 1000);\n    }\n  }\n};\n</script>\n\n<style scoped>\n.recipe-search {\n  padding: 20px;\n}\n\n.search-input {\n  margin-bottom: 30px;\n}\n\n.search-input input {\n  width: 70%;\n  padding: 10px;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n  margin-right: 10px;\n  font-size: 16px;\n}\n\n.search-input button {\n  padding: 10px 20px;\n  background-color: #42b983;\n  color: white;\n  border: none;\n  border-radius: 4px;\n  cursor: pointer;\n  font-size: 16px;\n}\n\n.search-input button:hover {\n  background-color: #368a68;\n}\n\n.loading-message, .no-results-message {\n  margin-top: 20px;\n  font-style: italic;\n  color: #666;\n}\n\n.recipe-list {\n  display: grid;\n  grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));\n  gap: 20px;\n  margin-top: 20px;\n}\n</style>"],"mappings":"AA2BA,OAAOA,UAAS,MAAO,kBAAkB,EAAE;;AAE3C,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,UAAU,EAAE;IACVF;EACF,CAAC;EACDG,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,eAAe,EAAE,EAAE;MACnBC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,KAAK;MACdC,QAAQ,EAAE,KAAI,CAAE;IAClB,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,aAAaA,CAAA,EAAG;MACd;MACA,IAAI,CAACH,OAAM,GAAI,IAAI;MACnB,IAAI,CAACC,QAAO,GAAI,IAAI;MACpB;MACAG,UAAU,CAAC,MAAM;QACf,IAAI,CAACL,OAAM,GAAI,EAAE,EAAE;QACnB,IAAI,CAACC,OAAM,GAAI,KAAK;QACpBK,OAAO,CAACC,GAAG,CAAC,gBAAgB,EAAE,IAAI,CAACR,eAAe,CAAC;QACnD;QACA,IAAI,IAAI,CAACA,eAAe,CAACS,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,MAAM,CAAC,EAAE;UACrD,IAAI,CAACT,OAAM,GAAI,CACX;YAAEU,EAAE,EAAE,CAAC;YAAEC,KAAK,EAAE,mBAAmB;YAAEC,WAAW,EAAE,CAAC,SAAS,EAAE,iBAAiB,CAAC;YAAEC,YAAY,EAAE,SAAS;YAAEC,KAAK,EAAE;UAAkC,CAAC,EACrJ;YAAEJ,EAAE,EAAE,CAAC;YAAEC,KAAK,EAAE,kBAAkB;YAAEC,WAAW,EAAE,CAAC,UAAU,EAAE,iBAAiB,CAAC;YAAEC,YAAY,EAAE,SAAS;YAAEC,KAAK,EAAE;UAAkC,EACvJ;QACL;MACF,CAAC,EAAE,IAAI,CAAC;IACV;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}